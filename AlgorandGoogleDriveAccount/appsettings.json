{
  "App": {
    "Host": "https://google.biatec.io",
    "ClientId": "your-google-client-id",
    "ClientSecret": "your-google-client-secret",
    "StorageFolderName": "Biatec",
    "StorageFileName": "AVMAccount.dat",
    "ApplicationName": "Biatec MCP Server"
  },
  "AesOptions": {
    "Key": "your-32-character-encryption-key-here"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://google.biatec.io",
      "https://localhost:44305"
    ]
  },
  "CrossAccountProtection": {
    "Enabled": false,
    "RequireSecurityCheck": true,
    "SecurityCheckIntervalMinutes": 60,
    "AutoReportEvents": true,
    "EnableGranularConsent": false,
    "FilterInternalScopes": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
